{
    "name": "flask-backbone-todo",
    "version": "1.0.0",
    "description": "Flask and backbone todo app",
    "keywords": [
        "backbone",
        "frontend"
    ],
    "scripts": {
        "build": "browserify -t hbsfy ./js/src/main.js -o ./app/static/js/main.js",
        "watch": "watchify -t hbsfy ./js/src/main.js -o ./app/static/js/main.js -dv",
        "server": "source ./env/bin/activate && python run.py",
        "start": "npm run watch & npm run server & gulp"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:cfierro/todo.git"
    },
    "devDependencies": {
        "browserify": "^11.2.0",
        "watchify": "^3.4.0",
        "hbsfy": "^2.4.1",
        "gulp": "^3.9.0",
        "gulp-sass": "^2.1.1"
    },
    "dependencies": {
        "backbone": "^1.1.3",
        "jquery": "^2.1.4",
        "underscore": "^1.8.3",
        "handlebars": "^4.0.2"
    }
}
